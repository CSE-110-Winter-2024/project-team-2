<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/goals_container"
            android:name="edu.ucsd.cse110.successorator.app.ui.noGoals.NoGoalsFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout="@layout/fragment_no_goals" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/date_mock_container"
            android:name="edu.ucsd.cse110.successorator.app.ui.dateMock.DateMockFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout="@layout/fragment_date_mock" />
</androidx.constraintlayout.widget.ConstraintLayout>